#Trainee Table

CREATE TABLE CustomerTable(cust_Id VARCHAR2(10) PRIMARY KEY, 
					cust_Name VARCHAR2(20), 
					cust_Location VARCHAR2(20), 
					cust_Domain VARCHAR2(20));